#!/bin/bash -x

# Get rid of any remaining auto-generated files

make maintainer-clean || true

rm -rf \
    aclocal.m4 \
    autom4te.cache \
    bin \
    ChangeLog~ \
    config.log \
    config.rpath \
    config.status \
    configure \
    configure.ac~ \
    ltmain.sh \
    m4 \
    Makefile \
    Makefile.in~ \
    Makefile.in \
    po \
    src/configure.ac \
    src/Makefile.in \
    docs/Doxyfile.in.bak \
    docs/Doxyfile

